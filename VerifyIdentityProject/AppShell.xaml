<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VerifyIdentityProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VerifyIdentityProject"
    Shell.FlyoutBehavior="Flyout"
    Title="VerifyIdentityProject">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Passport Data"
        ContentTemplate="{DataTemplate local:DG1Page}"
        Route="DG1Page"
        IsVisible="False" />

</Shell>
